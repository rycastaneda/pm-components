{
  "name": "category-selector",
  "version": "0.1.0",
  "description": "Module to allow users to select categories",
  "engines": {
    "node": ">=4",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "dev": "webpack-dev-server --progress --profile --colors",
    "lint": "eslint src",
    "fix": "eslint src test --fix",
    "test": "mocha tests/tests.config.js tests/*.spec.js",
    "ci": "npm run lint && cross-env DEBUG_CALLS=true npm run test"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-json-api": "^1.5.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.3.0",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.0.2",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  }
}
